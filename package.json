{
  "name": "@appbuckets/react-bucket",
  "version": "0.3.0-alpha.23",
  "description": "React UI Framework",
  "keywords": [
    "react",
    "ui",
    "style",
    "components"
  ],
  "main": "dist/all.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "src/styles",
    "dist",
    "README",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "author": {
    "name": "Marco Cavanna",
    "email": "marco@appbuckets.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:marcocavanna/react-bucket.git"
  },
  "bugs": {
    "url": "https://github.com:marcocavanna/react-bucket/issues"
  },
  "homepage": "https://github.com:marcocavanna/react-bucket#readme",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build:fontawesome && npm run build",
    "prebuild:webpack": "rimraf dist && npm run lint && npm run test:coverage",
    "build": "cross-env NODE_ENV=production npm run build:webpack",
    "build:dev": "cross-env NODE_ENV=development npm run build:webpack",
    "build:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack.config.ts --progress",
    "postbuild:webpack": "npm run build:declaration",
    "postbuild:dev": "npm run prettier",
    "build:declaration": "tsc -p tsconfig.declaration.json",
    "build:fontawesome": "node build.fontawesome.js",
    "build:style": "node build.styles.js",
    "build:storybook": "build-storybook",
    "start:storybook": "start-storybook -p 57000",
    "start:styleguide": "styleguidist server",
    "start:docs": "cross-env DOCS=1 start-storybook --docs",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc --write \"dist/**/*{.js,.ts,.css}\"",
    "plop": "plop"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@svgr/core": "^5.4.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.1",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/typescript": "^2.0.0",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-declaration-sorter": "^5.1.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "plop": "^2.7.4",
    "postcss": "^7.0.32",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-merge-rules": "^4.0.3",
    "postcss-momentum-scrolling": "^2.7.17",
    "postcss-reporter": "^6.0.1",
    "postcss-single-charset": "^2.0.0",
    "postcss-single-line": "^1.0.0",
    "postcss-sort-media-queries": "^1.6.24",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript": "^1.16.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-styleguidist": "^11.0.8",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svgo": "^1.3.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yaml": "^1.10.0"
  },
  "dependencies": {
    "@appbuckets/react-ui-core": "0.0.2",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@popperjs/core": "2.4.2",
    "@semantic-ui-react/event-stack": "3.1.1",
    "clsx": "^1.1.1",
    "lodash": "^4.17.15",
    "rc-table": "7.8.1",
    "react-popper": "2.2.3"
  }
}
