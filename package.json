{
  "name": "@appbuckets/react-bucket",
  "version": "0.3.0-alpha.1",
  "description": "React UI Framework",
  "keywords": [
    "react",
    "ui",
    "style",
    "components"
  ],
  "main": "dist/lib/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "author": {
    "name": "Marco Cavanna",
    "email": "marco@appbuckets.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:marcocavanna/react-bucket.git"
  },
  "bugs": {
    "url": "https://github.com:marcocavanna/react-bucket/issues"
  },
  "homepage": "https://github.com:marcocavanna/react-bucket#readme",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist && npm run lint",
    "prebuild:dev": "rimraf dist",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=production webpack --config webpack.config.ts --progress",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" NODE_ENV=development webpack --config webpack.config.ts --progress",
    "build:declaration": "tsc -p tsconfig.declaration.json",
    "build:storybook": "build-storybook",
    "postbuild": "npm run build:declaration",
    "start:storybook": "start-storybook",
    "start:docs": "cross-env DOCS=1 start-storybook --docs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts'"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.0",
    "@types/jest": "^25.2.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-declaration-sorter": "^5.1.2",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "postcss-single-line": "^1.0.0",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@appbuckets/react-ui-core": "^0.0.1-beta.1",
    "clsx": "^1.1.1"
  }
}
