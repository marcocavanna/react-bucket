{
  "name": "@appbuckets/react-bucket",
  "version": "0.0.2",
  "description": "React UI Framework",
  "main": "dist/react/index.js",
  "scripts": {
    " [ Production ] ": "-",
    "build": "npm run build:js:prod",
    "build:js:prod": "cross-env NODE_ENV=production babel src/react -d dist/react",
    "prebuild": "rimraf dist",
    "postbuild": "npm run dtsbuild && npm run fa:icon:generator && npm run build:style",
    "  ": "-",
    " [ Development ] ": "-",
    "build:dev": "npm run build:js:dev",
    "build:js:dev": "cross-env NODE_ENV=development babel src/react -d dist/react",
    "prebuild:dev": "rimraf dist",
    "postbuild:dev": "npm run dtsbuild && npm run fa:icon:generator && npm run build:style",
    "   ": "-",
    " [ Style ] ": "-",
    "build:style": "npm run build:style:sass && npm run build:style:postcss && npm run build:style:cleancss && npm run build:style:mqpacker && npm run build:style:copyfa",
    "build:style:sass": "node-sass --output-style expanded --source-map true src/scss/react-bucket.scss dist/style/react-bucket.css",
    "build:style:postcss": "postcss --use autoprefixer postcss-modules --map false --output dist/style/react-bucket.css dist/style/react-bucket.css",
    "build:style:cleancss": "cleancss -o dist/style/react-bucket.css.tmp dist/style/react-bucket.css",
    "build:style:mqpacker": "mqpacker --sort dist/style/react-bucket.css.tmp > dist/style/react-bucket.min.css && rm dist/style/react-bucket.css.tmp",
    "build:style:copyfa": "cpy \"**/*\" \"../../../dist/style/fontawesome/webfonts\" --cwd=src/fontawesome/webfonts --parents",
    "    ": "-",
    " [ Docs ] ": "-",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.docs.config.js --open",
    "build:docs": "cross-env NODE_ENV=production webpack --config webpack.docs.config.js --colors --display-error-details --progress",
    "build:component-info": "gulp build:docs:docgen",
    "     ": "-",
    " [ Utility ] ": "-",
    "test": "cross-env NODE_ENV=test node -r @babel/register ./node_modules/karma/bin/karma start karma.conf.babel.js",
    "lint": "eslint src",
    "try": "cross-env BABEL_CONFIG=api babel-node",
    "fa:icon:generator": "node ./src/fontawesome/icon-module/generator.js && cross-env NODE_ENV=production babel src/fontawesome -d dist/fontawesome --ignore src/fontawesome/icon-module/generator.js && cpy \"src/fontawesome/icon-module/fa-icon.d.ts\" \"dist/fontawesome/icon-module\"",
    "dtsbuild": "cpy \"**/*.d.ts\" \"../../dist/react\" --cwd=src/react --parents"
  },
  "repository": {
    "type": "git",
    "url": "git+https://marcocavanna@bitbucket.org/marcocavanna/react-bucket.git"
  },
  "keywords": [
    "webpack",
    "react",
    "javascript",
    "framework",
    "ui"
  ],
  "author": "Marco Cavanna",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@stardust-ui/react-component-ref": "^0.40.0",
    "@types/react": "^16.8.19",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-console-source": "^2.0.4",
    "babel-plugin-groundskeeper-willie": "^1.3.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-require": "^3.1.1",
    "babel-plugin-transform-react-handled-props": "^2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "circular-dependency-plugin": "^5.2.0",
    "clean-css-cli": "^4.3.0",
